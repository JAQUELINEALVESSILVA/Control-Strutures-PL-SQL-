DECLARE

  V_SEACRH_NUMBER NUMBER := 32453 ;
  V_IS_PRIME Boolean := TRUE;

BEGIN

  FOR X IN 2..V_SEACRH_NUMBER - 1 LOOP

    IF V_SEACRH_NUMBER MOD X = 0 THEN

      DBMS_OUTPUT.PUT_LINE (V_SEACRH_NUMBER || ' IS NOT A PRIME NUMBER..');
      
      V_IS_PRIME := FALSE;
      GOTO END_POINT;
  
  END IF;
  
  END LOOP;
  
  IF V_IS_PRIME THEN
   DBMS_OUTPUT.PUT_LINE (V_SEACRH_NUMBER || ' IS A PRIME NUMBER..');
   END IF;
   
   <<END_POINT>>
     DBMS_OUTPUT.PUT_LINE ('CHECK COMPLETE..');
     END;